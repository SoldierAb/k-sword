{
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules":false,
          "useBuiltIns": "usage",
          "corejs": {
            "version": 3,
            "proposals": true // 使用尚在“提议”阶段特性的 polyfill
          }
        }
      ],
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/proposal-class-properties"
    ]
}
