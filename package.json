{
  "name": "k-sword",
  "version": "1.0.3",
  "description": "Javascript Library base on Typescript",
  "main": "lib/k-sword.umd.js",
  "module": "lib/k-sword.esm.js",
  "types": "src/index.d.ts",
  "scripts": {
    "dev": "rollup -w -c --environment FORMAT:esm",
    "dev:doc": "vuepress dev docs",
    "build": "gulp",
    "build:doc": "vuepress build docs",
    "lint": "eslint src --fix",
    "test": "jest --coverage",
    "release": "standard-version",
    "commit": "git add . && git-cz",
    "postinstall": "node scripts/post-install || echo \"ignore\""
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*./src/**/*.{js,ts,json,md}": [
      "prettier --write",
      "npm run lint"
    ]
  },
  "author": "cgj",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@microsoft/api-extractor": "^7.9.13",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.5.0",
    "fs-extra": "^9.0.1",
    "git-cz": "^4.7.0",
    "gulp": "^4.0.2",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rollup": "^2.23.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "rollup-plugin-uglify": "^6.0.4",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.1.3",
    "tslib": "^2.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-config-standard-plus": "^2.3.0",
    "typescript": "^3.9.7",
    "util": "^0.12.3",
    "vuepress": "^1.5.3"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.1.0",
    "core-js": "^3.6.5",
    "node-emoji": "^1.10.0",
    "rollup-plugin-node-polyfills": "^0.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SoldierAb/k-sword.git"
  },
  "bugs": {
    "url": "https://github.com/SoldierAb/k-sword/issues"
  },
  "homepage": "https://github.com/SoldierAb/k-sword#readme"
}
